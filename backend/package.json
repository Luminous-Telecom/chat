{
  "name": "backend",
  "version": "2.0.0",
  "description": "izing.open.io - sistema de atendimentos multicanal",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "watch": "tsc -w",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "dev:server": "ts-node-dev --inspect=9229 --respawn --transpile-only --ignore node_modules src/server.ts",
    "pretest": "NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "test": "NODE_ENV=test jest",
    "posttest": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:migrate": "rm -Rf dist && npm run build && npx sequelize db:migrate",
    "db:seed": "npx sequelize db:seed:all",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --only chat-backend-dev --env development",
    "pm2:prod": "pm2 start ecosystem.config.js --only chat-backend-prod --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "clear-sessions": "ts-node src/utils/clearCorruptedSessions.ts",
    "session-monitor": "ts-node src/commands/sessionMonitor.ts",
    "session-diagnostics": "ts-node src/commands/sessionDiagnostics.ts",
    "session-health": "ts-node src/commands/sessionDiagnostics.ts health",
    "session-diagnose": "ts-node src/commands/sessionDiagnostics.ts diagnose",
    "session-restart": "ts-node src/commands/sessionDiagnostics.ts restart",
    "session-monitor-continuous": "ts-node src/commands/sessionDiagnostics.ts monitor",
    "session-fix-all": "ts-node src/commands/sessionDiagnostics.ts fix-all",
    "campaign-ack-diagnostics": "ts-node src/commands/campaignAckDiagnostics.ts",
    "test-campaign-ack": "node src/commands/testCampaignAckSimple.js",
    "debug-campaign-ack": "node src/commands/debugCampaignAck.js",
    "check-campaign-status": "node src/commands/checkCampaignStatus.js"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "Durans (lumardyelson@gmail.com)",
  "private": true,
  "license": "AGPL-3.0-or-later",
