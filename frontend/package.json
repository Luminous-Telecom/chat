{
  "name": "lumi-suite",
  "version": "1.0.0",
  "description": "Bot Multiatendimento, multicanais e multiempresas.",
  "productName": "LUMI SUITE",
  "author": "Durans <lumardyelson@gmail.com>",
  "private": false,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,ts}\"",
    "test": "echo \"No test specified\" && exit 0",
    "build": "quasar build",
    "dev": "quasar dev",
    "start": "quasar dev",
    "pm2:dev": "pm2 start ../ecosystem.config.js --only chat-frontend-dev --env development",
    "pm2:prod": "npm run build && pm2 start ../ecosystem.config.js --only chat-frontend-prod --env production",
    "pm2:stop": "pm2 stop ../ecosystem.config.js --only chat-frontend-dev,chat-frontend-prod",
    "pm2:delete": "pm2 delete ../ecosystem.config.js --only chat-frontend-dev,chat-frontend-prod",
    "pm2:logs": "pm2 logs --only chat-frontend-dev,chat-frontend-prod",
    "pm2:monit": "pm2 monit --only chat-frontend-dev,chat-frontend-prod"
  },
  "dependencies": {
